# Uncomment the next line to define a global platform for your project
platform :ios, '8.0'

target 'TravelImpression' do

pod 'AFNetworking'  # 网络请求
pod 'SDWebImage'   # 下载图片
pod 'SVProgressHUD'  # 加载提示
pod 'MJRefresh'  # 下拉刷新
pod 'IQKeyboardManager' # 键盘管理
pod 'RTRootNavigationController'  # 导航控制器
pod 'Masonry'      # 自动布局
pod 'LFImagePickerController' #图片选择器
pod 'JSQMessagesViewController' #聊天布局
pod 'FMDB'         # 数据库操作

pod 'UMengUShare/UI' # 友盟分享
pod 'UMengUShare/Social/Sina' # 友盟分享集成新浪微博
pod 'UMengUShare/Social/WeChat' # 友盟分享集成微信
pod 'UMengUShare/Social/QQ' # 友盟分享集成QQ

pod 'AVOSCloud'               # 数据存储、短信、云引擎调用等基础服务模块
pod 'AVOSCloudIM'             # 实时通信模块
pod 'AVOSCloudCrashReporting' # 错误报告模块

end
